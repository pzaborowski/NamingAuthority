    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <ns2:status rdf:resource="http://www.opengis.net/def/status/valid"/>
    <skos:broader rdf:resource="http://www.opengis.net/def/docs/is"/>
    <skos:prefLabel xml:lang="en">OGC Moving Features Encoding Extension - JSON</skos:prefLabel>
    <skos:altLabel xml:lang="en">Moving Features Encoding Extension - JSON</skos:altLabel>
    <skos:altLabel xml:lang="en">19-045r3</skos:altLabel>
