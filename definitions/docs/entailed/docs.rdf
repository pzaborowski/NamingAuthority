    <skos:prefLabel xml:lang="en">GeoAPI 3.0 Implementation Standard with corrigendum</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <ns2:doctype rdf:resource="http://www.opengis.net/def/doc-type/isc"/>
