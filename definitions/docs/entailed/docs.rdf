    <ns2:doctype rdf:resource="http://www.opengis.net/def/doc-type/is"/>
    <skos:prefLabel xml:lang="en">OGC GML Application Schema  Coverages : GRIB2 Coverage Encoding Profile</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.opengis.net/def/docs/16-097">
    <skos:inScheme rdf:resource="http://www.opengis.net/def/docs"/>
    <ns3:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2017-10-03</ns3:created>
