    <ns2:doctype rdf:resource="http://www.opengis.net/def/doc-type/dp"/>
    <skos:definition>This OpenGIS(r) document describes the API for two web services capable of generating several simplified data formats including GeoRSS and the Basic XML Feature Schema(BXFS). </skos:definition>
    <skos:inScheme rdf:resource="http://www.opengis.net/def/docs"/>
    <ns1:creator>Panagiotis (Peter) A. Vretanos</ns1:creator>
