  </rdf:Description>
  <rdf:Description rdf:about="http://www.opengis.net/def/docs/15-048r3">
    <rdfs:seeAlso rdf:resource="https://portal.ogc.org/files/?artifact_id=65420"/>
    <ns1:creator>Jeff Harrison</ns1:creator>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <ns2:doctype rdf:resource="http://www.opengis.net/def/doc-type/per"/>
