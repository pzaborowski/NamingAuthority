  <rdf:Description rdf:about="http://www.opengis.net/def/docs/13-084r2">
    <skos:inScheme rdf:resource="http://www.opengis.net/def/docs"/>
    <skos:broader rdf:resource="http://www.opengis.net/def/docs/isx"/>
    <ns2:status rdf:resource="http://www.opengis.net/def/status/valid"/>
