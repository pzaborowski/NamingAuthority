  <rdf:Description rdf:about="http://www.opengis.net/def/docs/09-025r1">
    <skos:prefLabel xml:lang="en">OpenGIS Web Feature Service 2.0 Interface Standard (also ISO 19142)</skos:prefLabel>
    <skos:altLabel xml:lang="en">Web Feature Service 2.0 Interface Standard (also ISO 19142)</skos:altLabel>
