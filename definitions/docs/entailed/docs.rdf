  </rdf:Description>
  <rdf:Description rdf:about="http://www.opengis.net/def/docs/09-104r1">
    <skos:prefLabel xml:lang="en">Draft for Candidate OpenGIS Web 3D Service Interface Standard</skos:prefLabel>
    <skos:inScheme rdf:resource="http://www.opengis.net/def/docs"/>
    <skos:broader rdf:resource="http://www.opengis.net/def/docs/dp"/>
    <ns2:doctype rdf:resource="http://www.opengis.net/def/doc-type/dp"/>
