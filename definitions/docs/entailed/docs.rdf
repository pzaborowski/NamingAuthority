    <ns2:status rdf:resource="http://www.opengis.net/def/status/valid"/>
    <ns1:creator>Gerhard GrÃ¶ger, Thomas H. Kolbe, Claus Nagel, Karl-Heinz HÃ¤fele</ns1:creator>
