  <rdf:Description rdf:about="http://www.opengis.net/def/docs/17-022">
    <skos:notation rdf:datatype="http://www.opengis.net/def/metamodel/ogc-na/doc_no">17-022</skos:notation>
    <skos:prefLabel xml:lang="en">OGC Testbed-13: NA001 Climate Data Accessibility for Adaptation Planning</skos:prefLabel>
    <ns2:doctype rdf:resource="http://www.opengis.net/def/doc-type/per"/>
    <skos:altLabel xml:lang="en">17-022</skos:altLabel>
    <skos:altLabel xml:lang="en">Testbed-13: NA001 Climate Data Accessibility for Adaptation Planning</skos:altLabel>
    <ns2:status rdf:resource="http://www.opengis.net/def/status/valid"/>
    <rdfs:seeAlso rdf:resource="http://docs.opengeospatial.org/per/17-022.html"/>
    <skos:definition>This Engineering Report describes all Testbed-13 activities relating to the Climate Data Accessibility for Adaptation Planning requirements of the National Aeronautics and Space Administration (NASA). It discusses relevant experiences made during implementation including recommendations to the sponsor, and provides resulting standards change requests to the appropriate working groups. Additionally, it develops best practices for data and model integration and serves as a guidance document to work with NASA Earth Science Data System (ESDS) working groups and externally provided data. The added value of this Engineering Report is to improve interoperability and to advance location-based technologies and realize innovations with regards to NASA Climate Data and NASA ESDS objectives.</skos:definition>
    <skos:broader rdf:resource="http://www.opengis.net/def/docs/per"/>
    <ns3:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-01-11</ns3:created>
    <ns1:creator>Guy Schumann</ns1:creator>
    <skos:inScheme rdf:resource="http://www.opengis.net/def/docs"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.opengis.net/def/docs/06-028r3">
    <skos:prefLabel xml:lang="en">Sensor Alert Service</skos:prefLabel>
    <skos:broader rdf:resource="http://www.opengis.net/def/docs/bp"/>
