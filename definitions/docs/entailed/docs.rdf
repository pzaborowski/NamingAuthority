  <rdf:Description rdf:about="http://www.opengis.net/def/docs/15-065r1">
    <skos:altLabel xml:lang="en">Testbed11 Referenceable Grid Harmonization Engineering Report</skos:altLabel>
    <skos:altLabel xml:lang="en">15-065r1</skos:altLabel>
    <ns2:status rdf:resource="http://www.opengis.net/def/status/valid"/>
    <ns2:doctype rdf:resource="http://www.opengis.net/def/doc-type/per"/>
    <skos:inScheme rdf:resource="http://www.opengis.net/def/docs"/>
    <skos:definition>This Engineering Report is a deliverable of the Testbed-11 Urban Climate Resilience (UCR) Thread. The UCR Thread responds to the urgent need to make climate information and related data readily available for the public and government decision makers to prepare for changes in the Earths climate. An important set of a data sources that will play an important role in detecting changes due to climate effects are a wide array of remote imaging systems.</skos:definition>
    <skos:prefLabel xml:lang="en">OGC Testbed11 Referenceable Grid Harmonization Engineering Report</skos:prefLabel>
    <ns3:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2015-11-18</ns3:created>
    <ns1:creator>Eric Hirschorn, Peter Baumann</ns1:creator>
    <skos:broader rdf:resource="http://www.opengis.net/def/docs/per"/>
    <rdfs:seeAlso rdf:resource="https://portal.ogc.org/files/?artifact_id=64145"/>
