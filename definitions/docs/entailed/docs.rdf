    <ns2:status rdf:resource="http://www.opengis.net/def/status/valid"/>
    <skos:notation rdf:datatype="http://www.opengis.net/def/metamodel/ogc-na/doc_no">15-001r4</skos:notation>
    <skos:prefLabel xml:lang="en">OGC 3D Portrayal Service 1.0</skos:prefLabel>
  </rdf:Description>
