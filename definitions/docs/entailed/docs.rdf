    <rdfs:seeAlso rdf:resource="https://portal.ogc.org/files/?artifact_id=49000&amp;version=2"/>
    <rdfs:seeAlso rdf:resource="https://portal.ogc.org/files/?artifact_id=49000"/>
    <skos:notation rdf:datatype="http://www.opengis.net/def/metamodel/ogc-na/doc_no">12-066</skos:notation>
