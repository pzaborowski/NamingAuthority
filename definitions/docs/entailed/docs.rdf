  <rdf:Description rdf:about="http://www.opengis.net/def/docs/15-043r3">
    <skos:prefLabel xml:lang="en">Timeseries Profile of Observations and Measurements </skos:prefLabel>
    <rdfs:seeAlso rdf:resource="http://docs.opengeospatial.org/is/15-043r3/15-043r3.html"/>
    <ns1:creator>James Tomkins , Dominic Lowe </ns1:creator>
