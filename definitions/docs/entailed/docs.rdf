    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:seeAlso rdf:resource="https://portal.ogc.org/files/?artifact_id=39397"/>
    <skos:altLabel xml:lang="en">09-083r3</skos:altLabel>
    <ns1:creator>Adrian Custer</ns1:creator>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.opengis.net/def/docs/05-007r2">
