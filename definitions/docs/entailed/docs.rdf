    <skos:inScheme rdf:resource="http://www.opengis.net/def/docs"/>
    <ns3:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-12-19</ns3:created>
    <skos:altLabel xml:lang="en">15-120r5</skos:altLabel>
