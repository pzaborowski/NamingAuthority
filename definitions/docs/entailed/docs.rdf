  <rdf:Description rdf:about="http://www.opengis.net/def/docs/09-146r8">
    <ns1:creator>Peter Baumann, Eric Hirschorn, Joan MasÃ³</ns1:creator>
    <skos:altLabel xml:lang="en">Coverage Implementation Schema with Corrigendum</skos:altLabel>
    <skos:altLabel xml:lang="en">09-146r8</skos:altLabel>
    <skos:broader rdf:resource="http://www.opengis.net/def/docs/isc"/>
