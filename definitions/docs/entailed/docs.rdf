  </rdf:Description>
  <rdf:Description rdf:about="http://www.opengis.net/def/docs/19-021">
    <ns3:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-12-17</ns3:created>
    <ns2:status rdf:resource="http://www.opengis.net/def/status/valid"/>
