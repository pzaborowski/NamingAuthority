  <rdf:Description rdf:about="http://www.opengis.net/def/docs/10-087">
    <ns2:status rdf:resource="http://www.opengis.net/def/status/valid"/>
    <rdfs:seeAlso rdf:resource="https://portal.ogc.org/files/?artifact_id=40139"/>
