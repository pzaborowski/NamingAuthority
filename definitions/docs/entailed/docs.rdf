    <skos:prefLabel xml:lang="en">GML Encoding of Discrete Coverages (interleaved pattern)</skos:prefLabel>
    <skos:inScheme rdf:resource="http://www.opengis.net/def/docs"/>
    <ns2:doctype rdf:resource="http://www.opengis.net/def/doc-type/bp"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <ns3:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2007-05-17</ns3:created>
