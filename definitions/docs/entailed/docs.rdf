  <rdf:Description rdf:about="http://www.opengis.net/def/docs/09-144r2">
    <ns1:creator>Clemens Portele</ns1:creator>
    <ns2:doctype rdf:resource="http://www.opengis.net/def/doc-type/pol"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
